@{
    var topBar = new
                     {
                         BrandName = "Template",
                         BrandLink = "/",
                         Links = new []{
                                     new {
                                         Text = "Home",
                                         Url = "/"
                                     },
                                     new {
                                         Text = "About",
                                         Url = "/"
                                     }
                         }
                     };
}

<div class="navbar navbar-fixed-top navbar-inverse">
            <div class="navbar-inner">
                <div class="container-fluid">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar">
                        </span>
                        <span class="icon-bar">
                        </span>
                        <span class="icon-bar">
                        </span>
                    </a>
                    <a class="brand" href="@topBar.BrandLink">
                        @topBar.BrandName
                    </a>
                    <div class="nav-collapse pull-right">
                        <ul class="nav">
                            @foreach (var link in topBar.Links)
                            {
                                <li>
                                    <a href="@link.Url">
                                        @link.Text
                                    </a>
                                </li> 
                            }
                        </ul>
                        @Html.Partial("_LoginPartial")
                    </div>
                </div>
            </div>
        </div>